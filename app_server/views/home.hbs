<main>
    {{#if error}}
    <div class="alert alert-warning">{{error}}</div>
    {{else}}
    <div class="container-background">
        <div class="lambo-image">
            <div class="form-row filter">
                <div class="col-auto">
                    <label><strong>Location pick-up (city):</strong><input type="text" class="form-control filterCity"
                            placeholder="City"></label>
                </div>
                <div class="col-auto">
                    <label><strong>Date from:</strong><input type="date" class="form-control dateFrom" /></label>
                </div>
                <div class="col-auto">
                    <label><strong>Date to:</strong><input type="date" class="form-control dateTo" /></label>
                </div>
                <div class="col-auto">
                    <br><a class="filterlink" href="">
                        <button type="button" class="btn btn-light filterbutton">Filter</button>
                        </a>
                </div>

            </div>
        </div>
    </div>

    <br>

    <div class="container-fluid">
        <nav class="navbar navbar-category navbar-expand-lg justify-content-between">
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav"
                aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
                <text>Category<i class="fas fa-angle-down px-2 carousel-arrows"></i></text>
            </button>
            <div class="collapse navbar-collapse" id="navbarNav">
                <ul class="navbar-nav">
                    <li class="nav-item mx-md-4 active">
                        <a class="nav-link" href="/search?category=Hatchback">Hatchback<span class="sr-only">(current)</span></a>
                    </li>
                    <li class="nav-item mx-md-4">
                        <a class="nav-link" href="/search?category=Saloon">Saloon</a>
                    </li>
                    <li class="nav-item mx-md-4">
                        <a class="nav-link" href="/search?category=Caravan">Caravan</a>
                    </li>
                    <li class="nav-item mx-md-4">
                        <a class="nav-link" href="/search?category=Coupe">Coupe</a>
                    </li>
                    <li class="nav-item mx-md-4">
                        <a class="nav-link" href="/search?category=SUV">SUV</a>
                    </li>
                    <li class="nav-item mx-md-4">
                        <a class="nav-link" href="/search?category=Pick-up">Pick-up</a>
                    </li>
                </ul>
            </div>
        </nav>
    </div>


    <br>

